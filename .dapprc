
export DAPP_SRC=contracts

export DAPP_SOLC_VERSION=0.8.7

export DAPP_BUILD_OPTIMIZE=0
export DAPP_LINK_TEST_LIBRARIES=0
export DAPP_TEST_VERBOSITY=2
export DAPP_TEST_FUZZ_RUNS=100

export DAPP_REMAPPINGS=$(cat remappings.txt)
NEWLINE=$'\n'
case $NETWORK in
	polygon-mainnet)
		CONFIG="@config/=config/polygon-mainnet/"		
		;;
	
	*)
		echo -n "UNKNOW NETWORK"
		;;
esac

export DAPP_REMAPPINGS="$DAPP_REMAPPINGS$NEWLINE$CONFIG"
